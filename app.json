{
  "expo": {
    "name": "aiqfome-favorites",
    "slug": "aiqfome-favorites",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "sounds": []
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-aiqfome.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8ECFE"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "remote-notification"]
      },
      "bundleIdentifier": "com.kiondartel.aiqfome-favorites"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
